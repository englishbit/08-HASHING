#include<iostream>
using namespace std;
#define MAX 1000000
int hash[MAX];
void insert(int x){
    int i=0;
    int indx=x%MAX;
    while(hash[indx]!=-1){
        i++;
        if(i>=MAX)return;
        indx=(indx+i*i)%MAX;
        
    }
    
    hash[indx]=x;

}
int main(){
    int n;
    cin>>n;
    for(int i=0;i<MAX;i++)hash[i]=-1;

    for(int i=0;i<n;i++){
        int x;
        cin>>x;
        insert(x);
    }


    return 0;
}